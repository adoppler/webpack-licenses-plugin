{
  "name": "webpack-licenses-plugin",
  "version": "0.0.1",
  "description": "Generate a list of libraries and their licenses as part of your webpack build",
  "main": "webpack-licenses-plugin.js",
  "scripts": {
    "test": "NODE_ENV=test nyc ava",
    "test:watch": "NODE_ENV=test ava --watch",
    "eslint": "eslint .",
    "coverage": "nyc report --reporter=text-lcov"
  },
  "keywords": [
    "webpack",
    "license"
  ],
  "author": "adoppler",
  "license": "MIT",
  "homepage": "https://github.com/adoppler/webpack-licenses-plugin#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-eslint": "^6.1.0",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-ava": "^2.5.0",
    "eslint-plugin-import": "^1.9.2",
    "nyc": "^6.6.1"
  }
}
